<div class="panel panel-primary">
    <div class="panel-heading">
        Thông tin đầu vào của báo cáo
    </div>
    <div class="panel-body">
        <div class="col col-md-5">
            <h5>
                Đơn vị phát sinh dữ liệu :
            </h5>
            <select id="danhmuc_chinhanh">
                @foreach (var item in Model)
                {
                    <option class="form-control" value="@item.MA_DVI">@item.TEN_DVI</option>
                }
            </select>
        </div>
        <div class="col col-md-4">
            <h5>
                Ngày phát sinh dữ liệu :
            </h5>
            <input type="date" class="form-control" id="dateNgayPsinhDlieu" />
        </div>
        <div class="col col-md-3">
            <h5>
                Thao tác dữ liệu :
            </h5>
            <div class="btn-group">
                <input type="submit" value="Tải dữ liệu" class="btn btn-primary" id="btnGetData" />
                <input type="submit" value="In báo cáo" class="btn btn-info" id="btnCreateReport" />
            </div>
        </div>
    </div>
</div>